{
  "name": "@milon27/react-sidebar",
  "private": false,
  "version": "5.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build-tailwind": "npx tailwindcss -i ./src/index.css -o ./dist/react-sidebar.css --minify",
    "build": "tsc && vite build && pnpm build-tailwind",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "pnpm build"
  },
  "dependencies": {
    "react-icons": "5.0.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.2.2",
    "vite": "5.1.0",
    "vite-plugin-dts": "3.7.2"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "milon27",
    "email": "immilon27@gmail.com",
    "url": "https://milon27.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milon27/react-sidebar"
  },
  "keywords": [
    "typescript",
    "react-sidebar",
    "tailwind-sidebar",
    "nextjs-sidebar",
    "dashboard sidebar",
    "modern sidebar",
    "sidebar",
    "milon27"
  ],
  "license": "MIT"
}
