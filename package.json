{
    "version": "3.4.1",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build-tailwind": "npx tailwindcss -o ./dist/react-sidebar.css --minify",
        "build": "tsdx build && npm run build-tailwind",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "npm run build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "pub": "npm publish --access public"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-icons": ">=4"
    },
    "dependencies": {
        "react-icons": ">=4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "@milon27/react-sidebar",
    "author": {
        "name": "milon27",
        "email": "immilon27@gmail.com",
        "url": "https://milon27.com"
    },
    "keywords": [
        "typescript",
        "react-sidebar",
        "tailwind-sidebar",
        "nextjs-sidebar",
        "dashboard sidebar",
        "moderm sidebar",
        "sidebar"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/milon27/react-sidebar"
    },
    "module": "dist/react-sidebar.esm.js",
    "size-limit": [
        {
            "path": "dist/react-sidebar.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/react-sidebar.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@size-limit/preset-small-lib": "^7.0.8",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-info": "^5.3.21",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/addons": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@types/react": "^17.0.43",
        "@types/react-dom": "^17.0.14",
        "autoprefixer": "^10.4.4",
        "babel-loader": "^8.2.4",
        "husky": "^7.0.4",
        "next": "^12.1.1",
        "postcss": "^8.4.12",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-is": "^17.0.2",
        "react-router-dom": "^6.2.2",
        "size-limit": "^7.0.8",
        "tailwindcss": "^3.0.23",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3"
    }
}
